
type A {
	field0 = int8(10);

	int8 get() {
		return field0;
	}
}

type B {
	field0 = int16(10);
	field1 = int8(0);
	field2 = A();

	int16 get() {
		return field0 + field1 + field2.get();
	}

	void set_field(int8 x) {
		field1 = x;
	}
}

int16 main() {
	a = A();

	b = B();
	b.set_field(10);

	x = a.get();
	y = b.get();

	if x == 10 and y == 30 {
		return 0;
	} else {
		return 1;
	}
}
