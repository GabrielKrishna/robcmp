
type array_capsule {
	
	a = {10, 30, 10:2};
	b = {{1, 2, 3}, {4, 5, 6}};

	int16 getarr(int16 i) {
		return a[i];
	}

	int16 getmatrix(int16 i, int16 j) {
		return b[i][j];
	}
}

int16 main() {
	c = array_capsule();
	d = c.getarr(2);
	e = c.a[1];
	f = c.getmatrix(1, 1);
	g = c.b[0][0];

	if d == 10 and f == 5 {
		if e == 30 and g == 1 {
			return 0;
		}
	}
	return 1;
}

