{
  "build": {
    "f_cpu": "16000000L",
    "mcu": "atmega328p",
    "variant": "standard"
  },
  "debug": {
    "target_mcu": "atmega328p",
    "tools": {
      "robcmp-led": {
        "init_cmds": [
          "target remote $DEBUG_PORT",
          "$LOAD_CMDS"
        ],
        "port": ":1234",
        "server": {
          "package": "toolchain-robcmp",
          "executable": "bin/robcmp-simavr-debug"
        }
      },
      "robcmp-ssd1306": {
        "init_cmds": [
          "target remote $DEBUG_PORT",
          "$LOAD_CMDS"
        ],
        "port": ":1234",
        "server": {
          "package": "toolchain-robcmp",
          "executable": "bin/robcmp-simavr-debug"
        }
      },
      "robcmp-hd44780": {
        "init_cmds": [
          "target remote $DEBUG_PORT",
          "$LOAD_CMDS"
        ],
        "port": ":1234",
        "server": {
          "package": "toolchain-robcmp",
          "executable": "bin/robcmp-simavr-debug"
        }
      }
    }
  },
  "name": "ATmega328P/PA",
  "upload": {
    "maximum_ram_size": 2048,
