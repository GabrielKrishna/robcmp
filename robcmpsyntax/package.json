{
  "name": "robcmpsyntax",
  "displayName": "RobCmpSyntax",
  "description": "Robotic Compiler Language Syntax Coloring",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rob",
      "aliases": ["Robotic Compiler Language", "rob"],
      "extensions": [".rob"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rob",
      "scopeName": "source.rob",
      "path": "./syntaxes/rob.tmLanguage.json"
    }]
  }
}
