
type bitstruct {
	a = false;
	b = false;
	c = false;
	d = false;
	e = false;
	f = false;
	g = false;
	h = false;
	i = false;
	j = false;
	k = true;
}

void set_struct_kfield(bitstruct s) {
	s.k = false;
}

int16 main() {
	x = bitstruct();
	x.a = true;
	x.c = true;
	x.e = true;
	x.j = true;

	set_struct_kfield(x);

	if x.c and !x.k and x.j {
		return 0;
	} else {
		return 1;
	}
}

