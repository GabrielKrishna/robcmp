
int32 sum_matrix(int16[][] a);

int32 main() {
    // 3x3 matrix of one's
    x = {{int16(1):3}:3};

    zt = sum_matrix(x);
    if zt != 9 {
        return zt;
    }

    return 0;
}

int32 sum_matrix(int16[][] a) {
    i = 0;
    result = int32(0);
    while i < a.rows  {
        j = 0;
        while j < a.cols {
            result += a[i][j];
            j++;
        }
        i++;
    }
    return result;
}
