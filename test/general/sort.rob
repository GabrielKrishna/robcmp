/* vim: set tabstop=4:softtabstop=4:shiftwidth=4:noexpandtab
 * sorts the vector a and check the result
 */

void sort(int16[] a) {
	t = 0u;
	while t < a.size - 1 {
		s = t + 1;
		while s < a.size {
			if a[t] > a[s] {
				temp = a[t];
				a[t] = a[s];
				a[s] = temp;
			}
			s++;
		}
		t++;
	}
}

int16 main() {
	a = {int16(34), -5, 6, 0, 12, 100, 56, 22, 44, -3, -9, 12, 17, 22, 6, 11 };
	r = {int16(-9), -5, -3, 0, 6, 6, 11, 12, 12, 17, 22, 22, 34, 44, 56, 100 };
	
	sort(a);
	i = 0u;
	while i < a.size {
		if a[i] != r[i] {
			return 1;
		}
		i++;
	}
	return 0;
}

