int32 pass_array_pointer(int16[] b) {
    result = b[1] + b.size;
    return result;
}

int32 sum_array(int16[] a) {
    i = 0;
    result = int32(0);
    while i < a.size  {
        result += a[i];
        i++;
    }
    return result + pass_array_pointer(a);
}
