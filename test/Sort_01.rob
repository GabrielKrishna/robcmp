a = { 34, -5, 6, 0, 12, 100, 56, 22, 44, -3, -9, 12, 17, 22, 6, 11 };
int16 sort (int16 n)
{
	t = 0;
	s = 0;
	temp = 0;
	while (t < n - 1)
	{
		s = t + 1;
		while (s < n)
		{
			if ( a[t] > a[s] ) {
				temp = a[t];
				a[t] = a[s];
				a[s] = temp;
			}
			s++;
		}
		t++;
		//print t;
	}
	return 0;
}

int16 main()
{
	i = 0;
	while (i < 16){
		print a[i];
		i++;
	}
	print "\n";

	sort (16);
	i = 0;
	while(i < 16){
		print a[i];
		i++;
	}
}
