
/* This test case allocates a zeroed matrix in .bss region.
   It tests if the assembly func __do_clear_bss, called
   before main, is working properly.
 */

matrix = {{0u:128}:8};

int8 main() {
    row = 0u;
    while row < matrix.rows {
        col = 0u;
        while col < matrix.cols {
            if matrix[row][col] != 0 {
                return 1;
            }
            col++;
        }
        row++;
    }
    return 0;
}

