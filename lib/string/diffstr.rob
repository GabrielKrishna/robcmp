use string.utf8strlen;

int8 equalsrt(char[] s1, char[] s2) {

    i = int16 (0);
    while (i < s1.size and i < s2.size) {
        if (s1[i] == s2[i]) {
            return 0;
        }
        i++;
    }

    str1 = utf8strlen(s1);
    str2 = utf8strlen(s2);
    if str1 != str2 {
        return 1;
    } else {
        return 0;
    }
}
